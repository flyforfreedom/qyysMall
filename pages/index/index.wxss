/* pages/index/index.wxss */
.slide-swiper{
  width: 750rpx;
  height: 400rpx;
}
.slide-image,.slide-image navigator,.slide-image image{
  display: block;
  width: 100%;
  height: 100%;
}
.navAttr,.productList{
  overflow: hidden;
}
.navAttr>view{
  width:25%;
  height: 187.5rpx;
  text-align: center;
  float: left;
}
.navAttr navigator{
  display: block;
  height: 100%;
  width: 100%;
}
.navAttr image{
  display: inlien-block;
  width:100%;
  height:100%;
  position:relative;
  top:50%;
  transform: translateY(-50%);
}
.productList>view{
  width: 100%;
  text-align: center;
  float: left;
}
.productList image{
  display: block;
  margin: 0 auto;
  width: 100%;
  height: auto;
}